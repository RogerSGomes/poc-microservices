generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Professor {
  id         String   @id @default(uuid()) @map("professor_id")
  nome       String
  email      String   @unique
  senha      String
  created_at DateTime @default(dbgenerated("now()"))
  updated_at DateTime @default(dbgenerated("now()"))
}
